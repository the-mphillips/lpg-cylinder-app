{
  "name": "bwa-gas-reports-system",
  "version": "1.0.0",
  "description": "BWA GAS - LPG Cylinder Testing and Certification Management System",
  "private": true,
  "keywords": [
    "lpg",
    "cylinder",
    "testing",
    "certification",
    "nextjs",
    "react",
    "typescript",
    "trpc"
  ],
  "author": "Michael Phillips - BWA GAS",
  "license": "MIT",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "set SENTRY_SUPPRESS_INSTRUMENTATION_FILE_WARNING=1&& set SENTRY_SUPPRESS_GLOBAL_ERROR_HANDLER_FILE_WARNING=1&& next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "analyze": "cross-env ANALYZE=true next build",
    "clean": "rm -rf .next node_modules/.cache",
    "setup": "npm install && echo 'Please configure .env.local with Supabase credentials'",
    "test-env": "node -e \"console.log('Environment check:', {url: !!process.env.NEXT_PUBLIC_SUPABASE_URL, key: !!process.env.NEXT_PUBLIC_SUPABASE_ANON_KEY})\""
  },
